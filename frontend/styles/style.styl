// .header
// .body
// .control
.btnBack
	background-image url('/img/button_bg_white_left.png')
	background-repeat no-repeat
	padding 0
	width 71px
	height 29px
	outline none
	border-radius: 9px
	text-align left 
	padding-left 5px
	&:hover
		background-image url('/img/button_bg_orange_left.png')
		span
			color white
		
.btnForth
	background-image url('/img/button_bg_white_right.png')
	background-repeat no-repeat
	padding 0
	width 71px
	height 29px
	outline none
	border-radius 9px
	text-align right 
	padding-right 5px
	font-weight:700
	&:hover
		background-image url('/img/button_bg_orange_right.png')
		span
			color white

.btnStore
	background-image url('/img/button_bg_white_right.png')
	background-repeat no-repeat
	background-size 100%
	width 135px
	height 29px
	outline none
	border-radius 9px
	text-align right 
	padding-right 5px
	font-weight:700
	float right 
	&:hover
		background-image url('/img/button_bg_orange_right.png')
		span
			color white

.btnSpForth
	margin-right 12px
	color darkgray
	font-weight 700
	
.btnSpBack
	margin-left 12px
	color darkgray
	font-weight 700
		
.btnSpStore
	margin-right 18px
	color orange
	font-weight 700
	
.arrowBtnForth 
	background-image url("/img/btn_ic_gray_right.png")
	width 12px
	height 8px
	margin 0
	display inline-block

.arrowBtnBack
	background-image url("/img/btn_ic_gray_left.png")
	width 12px
	height 8px
	margin 0
	display inline-block
	
.arrowBtnStore
	background-image url("/img/arrowSide.png")
	width 10px
	height 9px
	margin-right 10px
	display inline-block

	